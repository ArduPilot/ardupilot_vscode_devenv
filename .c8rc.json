{
  "all": true,
  "include": [
    "out/**/*.js"
  ],
  "exclude": [
    "out/test/**",
    "**/*.d.ts",
    "coverage/**",
    "node_modules/**"
  ],
  "reporter": [
    "text",
    "html",
    "lcov"
  ],
  "reports-dir": "coverage",
  "check-coverage": true,
  "lines": 70,
  "functions": 70,
  "branches": 70,
  "statements": 70,
  "watermarks": {
    "lines": [70, 90],
    "functions": [70, 90],
    "branches": [70, 90],
    "statements": [70, 90]
  }
}
